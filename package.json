{
  "name": "vue-live-preview",
  "version": "0.2.2",
  "description": "Live preview component for Vue.js and Nuxt.js",
  "main": "dist/vue-live-preview.umd.js",
  "module": "dist/vue-live-preview.esm.js",
  "unpkg": "dist/vue-live-preview.min.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js"
  },
  "author": "Julien Simonet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mons54/vue-live-preview"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "preview",
    "live preview",
    "live preview for vue"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-standalone": "^6.26.0",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "vue-codemirror": "^4.0.5",
    "vue-template-compiler": "^2.5.16"
  }
}
